    "terser-webpack-plugin": "^2.2.1",
    "tippy.js": "^6.2.5",
    "tmp": "^0.2.1",
    "url-loader": "^3.0.0",
    "walk": "^2.3.14",
    "webpack": "^4.44.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  }
}
